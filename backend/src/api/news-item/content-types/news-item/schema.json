{
  "kind": "collectionType",
  "collectionName": "news_items",
  "info": {
    "singularName": "news-item",
    "pluralName": "news-items",
    "displayName": "NewsItems",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "NavURL": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "LongText": {
      "type": "richtext"
    },
    "Classification": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::classification.classification"
    },
    "Category": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::news-item-category.news-item-category"
    },
    "Icon": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "Image": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "ShortVideo": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "videos"
      ]
    },
    "Video": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "videos"
      ]
    },
    "CreatedDate": {
      "type": "date"
    },
    "PublishedDate": {
      "type": "date"
    },
    "IsHighlighted": {
      "type": "boolean"
    },
    "IsAvailableOnInsights": {
      "type": "boolean"
    },
    "VueReferenceCode": {
      "type": "string",
      "default": "NewsItem"
    },
    "Title": {
      "type": "richtext",
      "required": true
    }
  }
}
